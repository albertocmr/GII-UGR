	@echo Pruebe a ejecutar el algoritmo recursivo con ./MergeSort.bin mergeSort.txt 12345 10000 20000 30000 40000 50000 60000 70000 80000 90000 100000
	@g++ -o InsertionSort.bin insertionsort.cpp -std=c++11
	@echo Pruebe a ejecutar el algoritmo recursivo con ./InsertionSort.bin insertionSort.txt 12345 10000 20000 30000 40000 50000 60000 70000 80000 90000 100000
	@g++ -o CountingSort.bin countingsort.cpp -std=c++11
	@echo Pruebe a ejecutar el algoritmo recursivo con ./CountingSort.bin countingSort.txt 12345 10000 20000 30000 40000 50000 60000 70000 80000 90000 100000
	@g++ -o Quicksort.bin Quicksort.cpp -std=c++11
	@echo Pruebe a ejecutar el algoritmo recursivo con ./Quicksort.bin Quicksort.txt 12345 10000 20000 30000 40000 50000 60000 70000 80000 90000 100000
